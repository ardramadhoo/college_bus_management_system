<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Report</title>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js">

    <!-- For navbar collapse -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>

    <!-- CSS -->
    <link rel="stylesheet" href="../../css/admin/reportprint.css">


    <!-- Gfonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Montserrat:wght@600;700&family=Roboto:wght@300;400&display=swap"
        rel="stylesheet">
    <!-- SweetAlert -->
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

</head>

<body onload="print()">

    <section id="middle-container">
        <div class="nitte_logo">
            <a href="/"><img src="../../images/nitte.png" alt="" ></a>
        </div>
        <h2 style="text-align:center;margin-top:1em;">Report</h2>
        <hr style="color:white">
        <hr style="color:white">
        <hr style="color:white">
        <div class="row">
            <table class="table table-striped">
                <tr>
                    <th>DESIGNATION</th>
                    <th>COUNT</th>
                </tr>
                <tr>
                    <td class="headcol">STUDENT</td>
                    <td><%= data.student_count %> </td>
                </tr>
                <tr>
                    <td class="headcol">FACULTY</td>
                    <td><%= data.faculty_count %> </td>
                </tr>
                <tr>
                    <td class="headcol">TOTAL</td>
                    <td><%= data.total_count %> </td>
                </tr>
            </table>
        </div>
        <hr style="color:white">
        <hr style="color:white">
        <hr style="color:white">
        <div class="row">
            <table class="table table-striped">
                <tr>
                    <td>NO. OF BUSES : </td>
                    <td><%= total_bus %> </td>
                </tr>
                <tr>
                    <td>NO. OF ROUTES : </td>
                    <td><%= total_routes %> </td>
                </tr>
                <tr>
                    <td>TOTAL CAPACITY OF BUSES : </td>
                    <td><%= total_capacity %> </td>
                </tr>
            </table>
        </div>
        <hr style="color:white">

    </section>
    <script type="text/javascript">
        function sleep(ms) {
            return new Promise(resolve => setTimeout(resolve, ms));
        }

        async function logout() {
            swal("You have been successfully logged out")
                .then((result) => {
                    window.location = "/"
                }).catch((err) => {
                    window.location = "/"
                });
        }
    </script>
</body>

</html>